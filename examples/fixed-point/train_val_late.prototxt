}
layers {
  name: "concat"
  bottom: "fc7-conv"
  bottom: "context_fc-conv"
  top: "concat"
  type: CONCAT
  concat_param {
    concat_dim: 1
  }
}
layers {
  bottom: "concat"
  top: "voc-fc8-conv"
  name: "voc-fc8-conv"
  type: CONVOLUTION
  blobs_lr: 10
  blobs_lr: 20
  weight_decay: 1
  weight_decay: 0
  convolution_param {
    num_output: 21
    kernel_size: 1
  }
}
layers {
name: "loss"
type: SOFTMAX_LOSS
bottom: "voc-fc8-conv"
bottom: "label"
}
layers {
name: "accuracy"
type: ACCURACY
bottom: "voc-fc8-conv"
bottom: "label"
top: "accuracy"
include: { phase: TEST }
}
